abbr{
	cursor: help;
}

h3{
	font-size: 20px;
	color: #4274bc;
}

h4{
	font-size: 16px;
	color: #3d5982;
	margin-bottom: 5px;
}

.svn-descr{
	color: #555;
	padding: 0;
	margin: 0;
	margin-bottom: 10px;
	font-size: 14px;
}

table.svn-files{
	border: 1px solid #ddd;
	width: 100%;
	border-collapse: collapse;
}

table.svn-files thead th{
	background: #eee;
	color: #000;
	text-align: left;
}

table.svn-files td, table.svn-files th {
	padding: 5px;
	vertical-align: middle;
	font-size: 13px;
}

table.svn-files td.action-cell{
	width: 50px;
}

a, a:link, a:visited{
	color: #33578c;
}

a:hover{
	color: #4274bc;
}

.mono{
	font-family: "Courier New";
	font-size: 80%;
}

.optional{
	font-size: 12px;
	color: #555;
}

.cent-cell{
	text-align: center;
}

td.error{
	color: #a00;
}

td.ok{
	color: #0a0;
}

td.red{
	background: #d74b4b;
}

td.green{
	background: #92eb66;
}

td.grey{
	background: #ccc;
}

.specific-actions{
	font-size: 13px;
	font-variant: small-caps;
	margin: 10px 0 4px 0;
	color: #666;
}

.notice{
	background: #daf6fb url(<%= "info.png" %>) no-repeat 10px center;
	color: #222;
	border: 1px solid #ccc;
	padding: 11px 20px 8px 32px;
	font-size: 14px;
	overflow: auto;
}

.error{
	background: #f1cfcf url(<%= "inactive.png" %>) no-repeat 10px center;
	color: #222;
	border: 1px solid #ccc;
	padding: 11px 20px 8px 32px;
	font-size: 14px;
	overflow: auto;
}

table.inner-table tr:first-child td:first-child{
	border-left: none !important;
}

table.inner-table tr:last-child td{
	border-bottom: none !important;
}

table.inner-table tr td:last-child{
	border-right: none !important;
}

.output{
	white-space: pre-wrap;
	font-family: monospace;
	display: block;
	background: #eee !important;
}

.show-details{
	margin-bottom: 10px;
	font-size: 80%;
	text-decoration: none;
}

#welcome-wrapper{
	overflow: auto;
	text-align: center;
	margin: 0 auto;
	width: 92%;
}

.welcome-box{
	
	padding: 10px;
	float:left;
	
	margin-right: 40px;
}

.welcome-box p{
	color: #333;
	font-size: 20px;
	font-weight: bold;
}

h1 a, h1 a:hover,h1 a:visited{
	color: #fff;
	text-decoration:none;
	cursor: pointer;
}

.inline{
  display:inline
}

.log_text{
	white-space: pre-wrap;
	font-family: monospace;
	display: block;
        width:700px;
        height:300px;
        background:#E9EFF8;
        overflow-y:scroll;
        
}

#lines{
  width:50px;
}

td.backup-finished{
	background: #c8f2c0;
}

td.backup-failed{
	background: #f48080;
}

td.backup-upload_failed{
	background: #f2c693;
}

td.backup-inprocess{
	background: #aeddf3;
}

td.backup-uploading{
	background: #e7d8f4;
}

abbr.gray{
	background: #ccc;
}

.db_list{
	list-style: none;
	padding: 0;
}


