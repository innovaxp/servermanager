body{
	margin:0;
	padding:0;
	font-family: Helvetica, "Trebuchet MS", Arial, sans-serif;
	font-size: 16px;
}

#header{
	padding:2px 20px;
	/*background: #33578c url(<%= asset_path "backgnd.png" %>) no-repeat right top;*/
	background: #33578c url(<%= asset_path "blueprint.jpg" %>) repeat right top;
}

#header h1{
	margin: 0;
	padding: 26px 85px;
	font-variant: small-caps;
	background: transparent url(<%= asset_path "terminal.png" %>) no-repeat 12px 15px;
	color: #f2f2f2;
	font-weight: lighter;
	letter-spacing: 2px;
	
	font-size: 30px;
}

#nav{
	height: 40px;
	background: #33578c url(<%= asset_path "blackprint.jpg" %>) repeat right top;
	color: #fff;
	border: none;
}

#nav ul{
	overflow:auto;
	margin: 0;
	padding: 0 20px;
	list-style: none;
}

#nav ul li{
	float: left;
	text-transform: uppercase;
	padding: 12px 10px;
	font-size: 14px;
}

#nav ul li a, #nav ul li a:visited{
	color: #fff;
	text-decoration: none;
}

#nav ul li a:hover{
	color: #e1e1e6;
	
}

#nav ul li:hover a{
	color: #222;
}
#nav ul li:hover{
	background: #eee;
}

#wrapper{
	background: #f6f6f6;
	overflow:auto;
}

#content{
	width: 800px;
	margin:0 auto;
	background: #fff;
	min-height: 550px;
	padding: 30px 60px;
	border-right: 1px solid #ccc;
	border-left: 1px solid #ccc;
}

#content h2, #content h1{
	color: #123a74;
	font-size: 28px;
}

#content .result{
	white-space: pre-wrap;
	font-family: monospace;
	display: block;

}

#content .match{
	font-weight: bold;
	background: #faffc7;
}

.filename_matches{
	font-size: 14px;
	font-weight: bold;
	background: #f1f1f1;
	padding: 5px 10px;
	font-family: Monospace;
}

.num-results{
	color: #33578c;
	font-weight: bold;
}

#footer{
	font-size: 13px;
	text-align: center;
	height: 50px;
	background: #33578c url(<%= asset_path "blueprint.jpg" %>) repeat right top;
	color: #f2f2f2;
	border: none;
	padding: 10px 0 0 0;
}

#footer a, #footer a:visited{
	color: #fff;
}

#footer a:hover{
	color: #e3e3e3;
}

input[type=submit], input[type=button], .button-link{
	border: 1px solid #222;
	font-size: 14px;
	padding: 5px 10px;
	color: #fff !important;
	background: #33578c;
	text-decoration: none;
}

input[type=submit]:hover, input[type=button]:hover, .button-link:hover{
	color: #33578c !important;
	background: #fff;
	cursor: pointer;
}

.field{
	margin: 10px 0;
}

.tab-group{
	overflow: auto;
	
}

.tab-active{
	border-bottom: 1px solid #fefefe !important;
	border-top: 4px solid #33578c !important;
	background: #fefefe !important;
	color: #000 !important;
	padding-top: 10px !important;
}

.tab-content{
	overflow: auto;
	background: #fefefe;
	border: 1px solid #888;
	padding: 30px;
	margin-top: -1px;
}

.tab{
	font-size: 12px;
	background: #f5f5f5;
	border: 1px solid #888;
	border-bottom: 1px solid #888;
	z-index: 50;
	float: left;
	text-align: center;
	padding: 10px 15px;
	padding-top: 13px;
	margin-right: 10px;
	cursor: pointer;
	color: #666;

}

#content table.index-table{
	border-collapse: collapse;
	width: 100%;
	font-size: 14px;
}

#content table.index-table td, #content table.index-table th{
	border: 1px solid #888;
	padding: 5px 8px;
}

#content table.index-table thead{
	background: #33578c;
	color: #fff;
}

